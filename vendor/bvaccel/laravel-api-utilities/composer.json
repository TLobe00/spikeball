{
    "name": "bvaccel/laravel-api-utilities",
    "description": "Laravel API Utilities",
    "license": "MIT",
    "authors": [
        {
            "name": "Don Herre",
            "email": "don.herre@bvaccel.com",
            "homepage": "https://bvaccel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/http": "^5.5",
        "illuminate/support": "^5.5",
        "illuminate/cache": "^5.5"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "BVAccel\\LaravelApiUtilities\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BVAccel\\LaravelApiUtilities\\LaravelApiUtilitiesServiceProvider"
            ],
            "aliases": {
                "ApiUtilities": "BVAccel\\LaravelApiUtilities\\Facades\\ApiUtilitiesFacade"
            }
        }
    }
}
